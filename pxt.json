{
    "name": "pxt-ble-data-connector",
    "version": "0.0.1",
    "description": "Bluetooth Low Energy (BLE) Data Connector",
    "dependencies": {
        "core": "*",
        "bluetooth": "*"
    },
    "files": [
        "README.md",
        "main.ts",
        "ble-extension.ts"
    ],
    "testFiles": [
        "test.ts"
    ],
    "public": true,
    "targetVersions": {
        "target": "6.0.28",
        "targetId": "microbit"
    },
    "supportedTargets": [
        "microbit"
    ],
    "preferredEditor": "tsprj",
    "yotta": {
        "config": {
            "microbit-dal": {
                "bluetooth": {
                    "open": 1,
                    "pairing_mode": 0,
                    "whitelist": 0
                }
            }
        }
    }
}
